{
    "key": "thanh-app-1837210622",
    "name": "Connect app Nodejs Azure - demo",
    "description": "Connect framework with Nodejs, deploy on Azure",
    "baseUrl": "https://crmprm-dev-env.azurewebsites.net",
    "authentication": {
        "type": "none"
    },
    "scopes": [
        "READ"
    ],
    "apiMigrations":{
        "signed-install": true
    },
    "modules": {
        "generalPages": [
            {
                "key": "hello-world-page-jira",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "Hello World"
                },
                "url": "/hello-world?issueKey={issue.key}",
                "conditions": [{
                    "condition": "user_is_logged_in"
                }]
            }
        ],
        "webPanels": [
            {
              "url": "/main?issueKey={issue.key}",
              "location": "atl.jira.view.issue.left.context",
              "name": {
                "value": "Reminder app"
              },
              "key": "thanh-reminder-web-right-panel"
            }
        ]
    }
}
